{
  "manifest_version": 3,
  "name": "Host Switcher",
  "version": "1.0",
  "description": "Switch the host of the current tab's URL.",
  "permissions": ["tabs", "activeTab", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
